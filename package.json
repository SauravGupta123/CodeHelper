{
  "name": "codehelper",
  "displayName": "CodeHelper",
  "description": "AI-powered code generation and analysis tool",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.103.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:codeHelper.helloWorld",
    "onCommand:codeHelper.analyzeCode",
    "onCommand:codeHelper.setApiKey",
    "onCommand:codeHelper.createWithAI",
    "onCommand:codeHelper.chatWithAI"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "codeHelper.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "codeHelper.analyzeCode",
        "title": "Analyze Code"
      },
      {
        "command": "codeHelper.setApiKey",
        "title": "Set Gemini API Key"
      },
      {
        "command": "codeHelper.createWithAI",
        "title": "Create with AI"
      },
      {
        "command": "codeHelper.chatWithAI",
        "title": "Chat with AI"
      },
      {
        "command": "codeHelper.openTestWebview",
        "title": "Open Test Webview"
      },
      {
        "command": "codeHelper.chatWithAI",
        "title": "Chat With AI"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "build:webview": "cd webview && npm run build",
    "dev:webview": "cd webview && npm run dev"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "dotenv": "^17.2.1"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.103.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.32.0",
    "typescript": "^5.9.2"
  }
}
